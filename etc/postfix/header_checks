# /^X-NextMX: auto$/ FILTER smtp:
# /^X-NextMX: (.*)$/ FILTER smtp:$1

/^Content-Transfer-Encoding:/i PREPEND X-pEp-Postfix-prepend: hub365_was_here

/^User-Agent:/ IGNORE
/^Disposition-Notification-To:/ IGNORE
/^Read-Receipt-To:/ IGNORE
/^Registered-Mail-Reply-Requested-By:/ IGNORE
/^Return-Receipt-Requested:/ IGNORE
/^Return-Receipt-To:/ IGNORE
/^X-Confirm-Reading-To:/ IGNORE

# /^X-pEpgate-Version: (.*)$/ FILTER smtp:[192.168.10.40]:25
# /^X-pEpgate-mode: encrypt$/ FILTER smtp:[192.168.10.40]:25
# /^X-pEpgate-mode: decrypt$/ FILTER smtp:[192.168.10.40]:25
# /^X-pEpgate-Version: (.*)$/ FILTER pepbot:foo
